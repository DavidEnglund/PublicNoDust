<?xml version="1.0" encoding="utf-8" ?>
<local:AccordionPane xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
		     xmlns:local="clr-namespace:Dustbuster;assembly=Dustbuster"
             x:Class="Dustbuster.WeatherPane">
	
	<StackLayout>
		<Label Text="Weather" FontSize="27" HorizontalOptions="Center"  Style="{DynamicResource labelStyle}" />
		<Button x:Name="CalendarButton" FontSize="17" HorizontalOptions="Center" Text="Go to Calendar pane" Clicked="OnCalendarButtonClicked" />
	</StackLayout>
</local:AccordionPane>

